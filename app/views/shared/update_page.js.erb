<%= render 'shared/update_flash' %>

<% if poops.empty? %>
  $('.poops').html("<%= escape_javascript empty %>");
<% else %>
  $('.poops').html("<%= escape_javascript(render poops)%>");
<% end %>

$('.pagination').replaceWith("<%= escape_javascript(paginate poops, :params => {:id => (defined?(id) ? id : nil), :action => (defined?(ajax) ? 'not_approved' : controller.action_name)} ) %>");
