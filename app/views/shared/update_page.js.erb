<%= render 'shared/update_flash' %>

<% if @poops.empty? %>
  $('.poops').html('<%= empty %>');
<% else %>
  $('.poops').html('<%= escape_javascript(render poops)%>');
<% end %>

// $.scrollTo( $('body'), 1500 );
$('.pagination').replaceWith('<%= escape_javascript(paginate poops, :params => {:id => nil, :action => (defined?(ajax) ? "not_approved" : controller.action_name)} ) %>');
